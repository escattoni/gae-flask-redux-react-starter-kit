{
  "name": "gae-flask-redux-react-starter-kit",
  "version": "1.0.0",
  "scripts": {
    "repl": "$(npm bin)/babel-node",
    "harmony": "node --harmony",
    "start": "$(npm bin)/webpack-dev-server --config browser_client/config/webpack.dev.js --port 8081 --history-api-fallback --hot --inline --no-info",
    "build": "$(npm bin)/webpack -p --config browser_client/config/webpack.build.js",
    "check": "npm run lint && npm run test:all",
    "lint": "$(npm bin)/eslint browser_client/",
    "test": "$(npm bin)/karma start browser_client/config/karma.unit.js --single-run",
    "test:watch": "$(npm bin)/karma start browser_client/config/karma.unit.js",
    "test:coverage": "$(npm bin)/karma start browser_client/config/karma.coverage.js --single-run",
    "test:e2e": "$(npm bin)/karma start browser_client/config/karma.e2e.js --single-run",
    "test:all": "$(npm bin)/karma start browser_client/config/karma.all.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/py-in-the-sky/gae-flask-redux-react-starter-kit.git"
  },

{
  "name": "gae-flask-redux-react-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "./user_interface/javascript/index.js",
  "scripts": {
    "start": "npm run build-dev && npm run serve",
    "build-dev": "NODE_ENV=development $(npm bin)/webpack --config webpack.config.js -d --display-reasons --display-chunks --display-error-details",
    "serve": "NODE_ENV=development $(npm bin)/webpack-dev-server --config webpack.config.js --hot --inline --no-info",
    "build": "NODE_ENV=production $(npm bin)/webpack -p --config webpack.config.js",
    "repl": "$(npm bin)/babel-node --stage 0",
    "check": "npm run lint && npm run test",
    "lint": "$(npm bin)/eslint user_interface/javascript/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/py-in-the-sky/gae-flask-redux-react-starter-kit.git"
  },
  "keywords": [
    "redux",
    "react"
  ],
  "author": "Ryan Wise",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/py-in-the-sky/gae-flask-redux-react-starter-kit/issues"
  },
  "homepage": "https://github.com/py-in-the-sky/gae-flask-redux-react-starter-kit#readme",
  "dependencies": {
    "humps": "^0.6.0",
    "immutable": "^3.7.4",
    "lodash": "^3.10.1",
    "react": "^0.13.3",
    "react-motion": "^0.2.7",
    "react-redux": "^2.0.0",
    "react-router": "^1.0.0-beta3",
    "react-tap-event-plugin": "^0.1.7",
    "redux": "^2.0.0",
    "redux-thunk": "^0.1.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.0.2",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.3.0",
    "jquery": "^2.1.4",
    "react-transform-catch-errors": "^0.1.1",
    "react-transform-webpack-hmr": "^0.1.2",
    "redbox-react": "^1.0.1",
    "redux-devtools": "^2.1.0",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }

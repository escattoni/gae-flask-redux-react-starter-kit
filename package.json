{
  "name": "gae-flask-redux-react-starter-kit",
  "version": "1.0.0",
  "scripts": {
    "repl": "$(npm bin)/babel-node --stage 0",
    "harmony": "node --harmony",
    "start": "NODE_ENV=development $(npm bin)/webpack-dev-server --config browser_client/config/webpack.dev.js --history-api-fallback --hot --inline --no-info",
    "build": "NODE_ENV=production $(npm bin)/webpack -p --config browser_client/config/webpack.build.js",
    "check": "npm run lint && npm run test:coverage",
    "lint": "$(npm bin)/eslint browser_client/",
    "test": "NODE_ENV=test $(npm bin)/karma start browser_client/config/karma.unit.js --single-run",
    "test:watch": "NODE_ENV=test $(npm bin)/karma start browser_client/config/karma.unit.js",
    "test:coverage": "NODE_ENV=test $(npm bin)/karma start browser_client/config/karma.coverage.js --single-run",
    "test:e2e": "NODE_ENV=test $(npm bin)/karma start browser_client/config/karma.e2e.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/py-in-the-sky/gae-flask-redux-react-starter-kit.git"
  },
  "keywords": [
    "redux",
    "redux-devtools",
    "react",
    "react-motion",
    "react-router",
    "immutable",
    "webpack",
    "webpack-dev-server",
    "babel"
  ],
  "author": "Ryan Wise",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/py-in-the-sky/gae-flask-redux-react-starter-kit/issues"
  },
  "homepage": "https://github.com/py-in-the-sky/gae-flask-redux-react-starter-kit#readme",
  "dependencies": {
    "humps": "^1.0.0",
    "immutable": "^3.7.4",
    "lodash": "^3.10.1",
    "react": "^0.14.0",
    "react-motion": "^0.3.0",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0-beta3",
    "redux": "^3.0.2",
    "redux-thunk": "^1.0.0",
    "velocity-animate": "^1.2.3",
    "velocity-react": "^1.1.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "chai": "^3.3.0",
    "empty": "^0.10.1",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^3.3.0",
    "isparta-instrumenter-loader": "^0.2.1",
    "jquery": "^2.1.4",
    "jsdom": "^6.5.1",
    "jsverify": "^0.7.1",
    "karma": "^0.13.11",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.1",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^2.1.0",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
}
